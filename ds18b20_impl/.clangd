CompileFlags:
  Add:
    - '-ferror-limit=0'
    - '-Wno-implicit-int'
    - --target=arm-none-eabi
    - -isystem/usr/lib/gcc/arm-none-eabi/14.2.1/include
    - -isystem/usr/lib/gcc/arm-none-eabi/14.2.1/include-fixed
    - -isystem/usr/lib/gcc/arm-none-eabi/14.2.1/../../../arm-none-eabi/include
  CompilationDatabase: build
Diagnostics:
  Suppress:
    - unused-includes
    - unknown_typename
    - unknown_typename_suggest
    - typename_requires_specqual
